<p id="notice"><%= notice %></p>

<h1>Administrative Processes</h1>

<table>
  <thead>
    <tr>
      <th>Benefit type</th>
      <th>Der</th>
      <th>Age</th>
      <th>Contribution time</th>
      <th>Movements</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administrative_processes.each do |administrative_process| %>
      <tr>
        <td><%= administrative_process.benefit_type %></td>
        <td><%= administrative_process.der %></td>
        <td><%= administrative_process.age %></td>
        <td><%= administrative_process.contribution_time %></td>
        <td><%= administrative_process.movements %></td>
        <td><%= administrative_process.status %></td>
        <td><%= link_to 'Show', administrative_process %></td>
        <td><%= link_to 'Edit', edit_administrative_process_path(administrative_process) %></td>
        <td><%= link_to 'Destroy', administrative_process, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if user_signed_in? %>
  <%= link_to 'New Administrative Process', new_administrative_process_path %>
  </br></br>
  <%= current_user.email %>
  </br>
  <%= link_to "Home", root_path %>
  </br>
  <%= link_to "Desconectar", destroy_user_session_path, method: :delete %>
<% end %>
