<%= simple_form_for @labor_process do |f| %>
  <%= f.input :process_category %>
  <%= f.input :process_number %>
  <%= f.input :distribution_date %>
  <%= f.input :stick %>
  <%= f.input :court %>
  <%= f.input :movements %>
  <%= f.input :status %>

  <div>
    <%= f.label :name %>
    <%= f.collection_select(:client_id, Client.all, :id, :name, prompt: true) %>
  </div>


  <%= f.button :submit %>
<% end %>
